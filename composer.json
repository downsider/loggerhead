{
  "name": "downsider/loggerhead",
  "description": "A turtle based solution to all your logging needs. Loggerhead receives and processes log messages from a queue",
  "license": "MIT",
  "homepage": "https://github.com/downsider/loggerhead",
  "require": {
    "php": ">=5.5.0",
    "downsider/puzzle-di": "^1.0",
    "silktide/reposition-sql": "^1.0",
    "silktide/reposition-clay": "^2.0",
    "silktide/syringe": "^1.0",
    "symfony/console": "~2.7.0",
    "robmorgan/phinx": "0.5.*",
    "silktide/queueball-redis": "^1.0",
    "silktide/teamster": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Downsider\\Loggerhead\\": "src",
      "Downsider\\Loggerhead\\Test\\": "test"
    }
  }
}
